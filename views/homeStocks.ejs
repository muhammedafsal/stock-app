		  </tr>
		</thead>
		<tbody>
		  <%if (companies.length >0){ %>
		  <% companies.forEach(function(row){ %>
		  <tr>
			<td><%= row.name %></td>
			<td><%= row.ltp %></td>
			<td><%= row.volume %></td>
		  </tr>
		  <% })}else{ %>
			<tr>
				<td colspan="3">No Record Found</td>
